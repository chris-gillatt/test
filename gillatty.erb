class Gillatty < Formula
  desc "Test"
  homepage "https://github.com/chris-gillatt/test"
  url "https://github.com/chris-gillatt/test/releases/download/0.0.<%= ENV["RELEASE_COUNT"] %>/gillatty-0.0.<%= ENV["RELEASE_COUNT"] %>.tar.gz"
  sha256 "<%= `sha256sum tars/gillatty-0.0.#{ENV["RELEASE_COUNT"]}.tar.gz | awk '{printf $1}'` %>"
  version "0.0.<%= ENV["RELEASE_COUNT"] %>"

  depends_on "curl"

  def install
    bin.install "test"
  end
end
